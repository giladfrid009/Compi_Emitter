int add(int a, int c) {
    return a + c;
}

int multiply(int a, int c) {
    return a * c;
}

bool iseven(int x) {
    return (x / 2) * 2 == x;
}

int getmax(int a, int b1, int c) {
    int max = a;
    max = b1 if (b1 > max) else max;
    max = c if (c > max) else max;
    return max;
}

int getmin(int a, int b1, int c) {
    int min = a;
    min = b1 if (b1 < min) else min;
    min = c if (c < min) else min;
    return min;
}

bool isprime(int x) {
    if (x < 2) {
        return false;
    }
    int i = 2;
    while (i <= x/2) {
        if ((x / i)*i == x) {
            return false;
        }
        i = i + 1;
    }
    return true;
}

